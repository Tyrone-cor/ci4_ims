# CodeIgniter 4 specific
writable/*
!writable/.gitkeep
!writable/*/
writable/*/index.html
writable/*/index.php

# Exclude everything in these directories
writable/cache/*
writable/debugbar/*
writable/logs/*
writable/session/*
writable/uploads/*

# But keep these files
!writable/cache/.gitkeep
!writable/debugbar/.gitkeep
!writable/logs/.gitkeep
!writable/session/.gitkeep
!writable/uploads/.gitkeep

# Environment files
.env
.env.backup
.env.*.backup

# IDE Files
/nbproject/
.idea/
*.sublime-project
*.sublime-workspace
.vscode/

# System Files
.DS_Store
Thumbs.db